{
  "name": "Test connector 1",
  "version": "0.0.3",
  "author": "VZ-1",
  "provider": {
      "name": "Commerce",
      "handler": {
        "graphql": {
          "endpoint": "https://venia.magento.com/graphql/"
        }
      },
      "transforms": [
        {
          "rename": {
            "mode": "bare | wrap",
            "renames": [
              {
                "from": {
                  "type": "Query",
                  "field": "compareList"
                },
                "to": {
                  "type": "Query",
                  "field": "productCompareList"
                }
              }
            ]
          }
        },
        {
          "filterSchema": {
            "mode": "bare | wrap",
            "filters": [
              "Query.!category",
              "Query.!customerOrders",
              "Query.!urlResolver",
              "Query.!wishlist"
            ]
          }
        },
        {
          "cache": [
            {
              "field": "Query.storeConfig",
              "invalidate": {
                "ttl": 3600
              }
            }
          ]
        }
      ]
    }
}